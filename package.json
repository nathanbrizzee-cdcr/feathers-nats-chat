{
  "name": "@cdcr/service-template",
  "description": "short  desc",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Nathan Brizzee",
    "email": "nathan.brizzee@cdcr.ca.gov"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.13.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "type": "commonjs",
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate:createdb": "node ./src/createDatabase.js",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.0-pre.36",
    "@feathersjs/authentication": "^5.0.0-pre.36",
    "@feathersjs/authentication-client": "^5.0.0-pre.36",
    "@feathersjs/authentication-local": "^5.0.0-pre.36",
    "@feathersjs/authentication-oauth": "^5.0.0-pre.36",
    "@feathersjs/configuration": "^5.0.0-pre.36",
    "@feathersjs/errors": "^5.0.0-pre.36",
    "@feathersjs/feathers": "^5.0.0-pre.36",
    "@feathersjs/knex": "^5.0.0-pre.36",
    "@feathersjs/koa": "^5.0.0-pre.36",
    "@feathersjs/schema": "^5.0.0-pre.36",
    "@feathersjs/socketio": "^5.0.0-pre.36",
    "@feathersjs/transport-commons": "^5.0.0-pre.36",
    "@feathersjs/typebox": "^5.0.0-pre.36",
    "dotenv": "^16.0.3",
    "feathers-nats-distributed": "github:nathanbrizzee-cdcr/feathers-nats-distributed",
    "feathers-sync": "^3.0.1",
    "get-port-please": "^3.0.1",
    "pg": "^8.9.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.0-pre.36",
    "@feathersjs/rest-client": "^5.0.0-pre.36",
    "@swc/core": "^1.3.31",
    "@swc/helpers": "^0.4.14",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.19",
    "axios": "^1.3.2",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
